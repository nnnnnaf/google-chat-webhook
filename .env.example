# Google Chat Webhook URLs 
# Format: GOOGLE_CHAT_WEBHOOKS={"project1":"URL1","project2":"URL2",...}
GOOGLE_CHAT_WEBHOOKS={"default":"https://chat.googleapis.com/v1/spaces/YOUR_SPACE_ID/messages?key=YOUR_KEY&token=YOUR_TOKEN"}

# Default webhook key (used when no specific project is requested)
DEFAULT_WEBHOOK_KEY=default

# Port for the local server
PORT=3001 